@if (TempData.ContainsKey(NotificationConstants.TempDataKeySuccess))
{
    <div class="alert alert-success alert-dismissible fade show" style="position: fixed; bottom: 0; right: 0; z-index: 9999" role="alert">
        <strong><i class="fas fa-info-circle"></i>@TempData[NotificationConstants.TempDataKeySuccess]</strong>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
}
else if (TempData.ContainsKey(NotificationConstants.TempDataKeyFail))
{
    <div class="alert alert-danger alert-dismissible fade show" style="position: fixed; bottom: 0; right: 0; z-index: 9999" role="alert">
        <strong><i class="fas fa-exclamation-triangle"></i>@TempData[NotificationConstants.TempDataKeyFail]</strong>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
}